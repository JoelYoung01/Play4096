{
	"name": "play4096",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node build",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint . --fix",
		"init-db": "node src/lib/init-db.js",
		"db:push": "drizzle-kit push --config drizzle.config.js",
		"db:generate": "drizzle-kit generate --config drizzle.config.js",
		"db:migrate": "drizzle-kit migrate --config drizzle.config.js",
		"db:studio": "drizzle-kit studio --config drizzle.config.js",
		"db:studio-deployed": "drizzle-kit studio --config drizzle.config.js --host 0.0.0.0 --port 3001",
		"db:seed": "node src/scripts/seed.js"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/better-sqlite3": "^7.6.12",
		"@types/node": "^22",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"pino-pretty": "^13.1.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"better-sqlite3",
			"esbuild",
			"sqlite3"
		]
	},
	"dependencies": {
		"@lucide/svelte": "^0.542.0",
