		"drizzle-kit": "^0.30.2",
		"drizzle-orm": "^0.40.0",
		"nodemailer": "^7.0.6",
		"pino": "^9.11.0",
		"sqlite3": "^5.1.7",
		"stripe": "^18.5.0"
	},
